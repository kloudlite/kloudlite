type Invitation @shareable {
  accepted: Boolean
  accountName: String!
  creationTime: Date!
  id: String!
  invitedBy: String!
  inviteToken: String!
  markedForDeletion: Boolean
  recordVersion: Int!
  rejected: Boolean
  updateTime: Date!
  userEmail: String
  userName: String
  userRole: Kloudlite_io__apps__iam__types_Role!
}

input InvitationIn {
  userEmail: String
  userName: String
  userRole: Kloudlite_io__apps__iam__types_Role!
}

