type ProjectSpec @shareable {
	displayName: String
	logo: String
	targetNamespace: String!
	accountName: String!
	clusterName: String!
}

input ProjectSpecIn {
	displayName: String
	logo: String
	targetNamespace: String!
	accountName: String!
	clusterName: String!
}

type Project @shareable {
	metadata: Metadata! @goField(name: "objectMeta")
	syncStatus: SyncStatus
	spec: ProjectSpec
	status: Status
	apiVersion: String
	kind: String
}

input ProjectIn {
	metadata: MetadataIn! @goField(name: "objectMeta")
	spec: ProjectSpecIn
	apiVersion: String
	kind: String
}

