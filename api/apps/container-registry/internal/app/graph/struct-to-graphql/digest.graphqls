type Digest @shareable {
  accountName: String!
  actor: String!
  creationTime: Date!
  deleting: Boolean!
  digest: String!
  id: String!
  length: Int!
  markedForDeletion: Boolean
  mediaType: String!
  recordVersion: Int!
  repository: String!
  size: Int!
  tags: [String!]!
  updateTime: Date!
  url: String!
}

type DigestEdge @shareable {
  cursor: String!
  node: Digest!
}

type DigestPaginatedRecords @shareable {
  edges: [DigestEdge!]!
  pageInfo: PageInfo!
  totalCount: Int!
}

