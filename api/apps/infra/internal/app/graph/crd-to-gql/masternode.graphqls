type MasterNodeSpec @shareable {
	provider: String!
	providerName: String!
	region: String!
	accountName: String!
	clusterName: String!
	config: String!
}

input MasterNodeSpecIn {
	provider: String!
	providerName: String!
	region: String!
	accountName: String!
	clusterName: String!
	config: String!
}

type MasterNode @shareable {
	metadata: Metadata! @goField(name: "objectMeta")
	syncStatus: SyncStatus
	spec: MasterNodeSpec
	status: Status
	apiVersion: String
	kind: String
}

input MasterNodeIn {
	metadata: MetadataIn! @goField(name: "objectMeta")
	spec: MasterNodeSpecIn
	apiVersion: String
	kind: String
}

