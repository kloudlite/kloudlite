type WorkerNodeSpec @shareable {
	clusterName: String!
	config: String!
	nodeIndex: Int
	pool: String!
	provider: String!
	accountName: String!
	edgeName: String!
	providerName: String!
	region: String!
	stateful: Boolean
}

input WorkerNodeSpecIn {
	clusterName: String!
	config: String!
	nodeIndex: Int
	pool: String!
	provider: String!
	accountName: String!
	edgeName: String!
	providerName: String!
	region: String!
	stateful: Boolean
}

type WorkerNode @shareable {
	metadata: Metadata! @goField(name: "objectMeta")
	syncStatus: SyncStatus
	spec: WorkerNodeSpec
	status: Status
	apiVersion: String
	kind: String
}

input WorkerNodeIn {
	metadata: MetadataIn! @goField(name: "objectMeta")
	spec: WorkerNodeSpecIn
	apiVersion: String
	kind: String
}

