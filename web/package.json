{
	"name": "kloudlite.github.io",
	"private": true,
	"version": "0.0.0",
	"homepage": "https://kloudlite.github.io",
	"type": "module",
	"scripts": {
		"storybook:dev": "storybook dev -p 6006",
		"storybook:build": "storybook build -o dist/static/storybook",
		"dev": "remix dev",
		"start": "sh scripts/run.sh",
		"build": "sh scripts/build.sh",
		"dbuild": "docker build --build-arg APP=${APP} . -t ghcr.io/kloudlite/web/${APP}:${IMAGE_TAG}",
		"dpush": "docker push ghcr.io/kloudlite/web/${APP}:${IMAGE_TAG}",
		"auth:build": "APP=auth pnpm build",
		"auth:dbuild": "APP=auth pnpm dbuild",
		"auth:dbnp": "APP=auth pnpm dbuild && pnpm dpush",
		"auth:start": "APP=auth pnpm start",
		"auth:dev": "APP=auth pnpm dev",
		"console:build": "APP=console pnpm build",
		"console:dbuild": "APP=console pnpm dbuild",
		"console:dbnp": "APP=console pnpm dbuild && pnpm dpush",
		"console:start": "APP=console pnpm start",
		"console:dev": "APP=console pnpm dev"
	},
	"dependencies": {
		"@jengaicons/react": "^0.7.0",
		"@madhouselabs/madrpc": "^1.0.9",
		"@radix-ui/react-checkbox": "^1.0.4",
		"@radix-ui/react-dialog": "^1.0.4",
		"@radix-ui/react-dropdown-menu": "^2.0.5",
		"@radix-ui/react-popover": "1.0.6",
		"@radix-ui/react-radio-group": "1.1.3",
		"@radix-ui/react-switch": "^1.0.3",
		"@radix-ui/react-toast": "^1.1.4",
		"@radix-ui/react-toggle-group": "^1.0.4",
		"@radix-ui/react-toolbar": "^1.0.4",
		"@radix-ui/react-tooltip": "^1.0.6",
		"@remix-run/node": "1.17.0",
		"@remix-run/react": "^1.17.0",
		"@remix-run/serve": "1.17.0",
		"@remix-run/testing": "^1.17.1",
		"axios": "^1.4.0",
		"classnames": "^2.3.2",
		"framer-motion": "^10.12.7",
		"graphql": "^16.7.1",
		"graphql-tag": "^2.12.6",
		"isbot": "^3.6.10",
		"prop-types": "^15.8.1",
		"react": "^18.2.0",
		"react-aria": "^3.25.0",
		"react-dom": "^18.2.0",
		"react-google-recaptcha-v3": "^1.10.1",
		"react-router-dom": "^6.10.0",
		"react-stately": "^3.22.0",
		"remix": "^1.17.0"
	},
	"devDependencies": {
		"@remix-run/dev": "1.17.0",
		"tailwindcss": "^3.3.2"
	}
}